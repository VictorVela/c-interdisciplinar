@model IEnumerable<WebApplication2.Models.Administrador>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.NomeAdministrador)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.SenhaAdministrador)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeAdministrador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SenhaAdministrador)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>|
                <a asp-action="ScriptCreate" asp-route-id="@item.Id">ScriptCreate</a>
            </td>
        </tr>
}
    </tbody>
</table>
